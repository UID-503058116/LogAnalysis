# OpenAI 配置
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1

# 模型配置
# EXTRACTION_MODEL: 用于从日志块中提取信息的模型（经济型）
# INTEGRATION_MODEL: 用于整合分析结果的模型（强大）
EXTRACTION_MODEL=gpt-4o-mini
INTEGRATION_MODEL=gpt-4o

# 分块配置
# CHUNK_SIZE: 每个日志块的最大字符数
# CHUNK_OVERLAP: 相邻块之间的重叠字符数
# MAX_CHUNKS: 最大分块数量限制
CHUNK_SIZE=4000
CHUNK_OVERLAP=200
MAX_CHUNKS=100

# MCP 服务器配置
# MCP_PORT: MCP 服务器监听端口
MCP_PORT=8000

# zai-sdk 可选配置
# ZAI_API_KEY: zai-sdk API 密钥（可选，用于网页搜索功能）
# ENABLE_WEB_SEARCH: 是否启用网页搜索功能
ZAI_API_KEY=your_zai_api_key_here
ENABLE_WEB_SEARCH=false

# ========== 流控配置 ==========
# TPM_LIMIT: TPM (每分钟事务数) 限制，None 或留空表示不限制
# RPM_LIMIT: RPM (每分钟请求数) 限制，None 或留空表示不限制
# RATE_LIMIT_BURST: 流控突发容量（令牌桶最大容量）
# ENABLE_RATE_LIMIT: 是否启用 TPM/RPM 流控
TPM_LIMIT=
RPM_LIMIT=60
RATE_LIMIT_BURST=10
ENABLE_RATE_LIMIT=true
